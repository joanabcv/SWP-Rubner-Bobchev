class Mensch:
    def __init__(self, name):
        self.name = name

    def vorstellen(self):
        print(self.name)


class Person(Mensch):
    def __init__(self, name, alter):
        super().__init__(name)
        self.alter = alter

    def info(self):
        print(self.alter)


class Schueler(Person):
    def __init__(self, name, alter, klasse):
        super().__init__(name, alter)
        self.klasse = klasse

    def schule(self):
        print(self.klasse)


s = Schueler("Joana ", 19, "5BHWII")
s.vorstellen()
s.info()
s.schule()
